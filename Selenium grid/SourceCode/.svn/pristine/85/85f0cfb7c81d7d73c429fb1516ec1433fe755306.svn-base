package joomla;

import org.openqa.selenium.By;

public class LoginPage {

	/*===================================Static Elements===================================*/
	public static final By txtUsername = By.id("mod-login-username");
	public static final By txtPassword = By.id("mod-login-password");
	public static final By btnLogin = By.xpath("//form[@id='form-login']//button");
	public static final By lblInvalidSecurityToken = By.xpath("//body[contains(text(),'The most recent request was denied because it contained an invalid security token. Please refresh the page and try again')]");

}
